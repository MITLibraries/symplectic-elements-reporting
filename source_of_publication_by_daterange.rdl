    <DataSet Name="ReportTarget">
      <Query>
        <DataSourceName>pubdata_datasource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@Group_IDs">
            <Value>=Parameters!Group_IDs.Value</Value>
          </QueryParameter>
          <QueryParameter Name="@User_IDs">
            <Value>=Parameters!User_IDs.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>--DECLARE @Group_IDs NVARCHAR(10) = '303,304,305';
--DECLARE @User_IDs NVARCHAR(10) = '1,10,25';

;WITH selectedGroups as
(SELECT CAST(value AS INT) AS GroupID 
			FROM STRING_SPLIT(@Group_IDs, ',')
),

Users as
(SELECT CAST(value AS INT) AS UserID
	FROM STRING_SPLIT(@User_IDs, ',')
)

select (select STRING_AGG(Grp.Name, '; ') from selectedGroups join [Group] Grp on grp.ID = selectedGroups.GroupID) GroupNames,
	(select STRING_AGG(Usr.[Computed Name Full], '; ') from Users join [User] Usr on Usr.ID = Users.UserID) UserNames
</CommandText>
      </Query>
