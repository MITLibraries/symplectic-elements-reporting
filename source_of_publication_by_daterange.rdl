        <Field Name="GroupNames">
          <DataField>GroupNames</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="UserNames">
          <DataField>UserNames</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
    </DataSet>
